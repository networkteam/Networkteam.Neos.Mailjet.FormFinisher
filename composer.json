{
    "description": "Form Finisher using mailjet",
    "type": "neos-package",
    "name": "networkteam/neos-mailjet-formfinisher",
    "require": {
        "neos/flow": "~6.0 || ~7.0",
        "neos/swiftmailer": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Networkteam\\Neos\\Mailjet\\FormFinisher\\": "Classes/"
        }
    },
    "suggest": {
        "neos/form-builder": "When using nodeType Networkteam.Neos.Mailjet.FormFinisher:Finisher.Mailjet the neos/form-builder package is needed."
    },
    "extra": {
        "neos": {
            "package-key": "Networkteam.Neos.Mailjet.FormFinisher"
        },
        "applied-flow-migrations": [
            "TYPO3.Form-20160601101500",
            "Neos.Twitter.Bootstrap-20161124204912"
        ]
    }
}